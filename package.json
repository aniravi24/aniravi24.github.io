{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && next-sitemap && touch out/.nojekyll && pnpm search-build",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "search-build": "pagefind --site .next/server/app --output-path out/_pagefind",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.1.7",
    "next-sitemap": "4.2.3",
    "nextra": "4.2.16",
    "nextra-theme-blog": "4.2.16",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "9.22.0",
    "eslint-config-next": "15.1.7",
    "gh-pages": "6.3.0",
    "pagefind": "1.3.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.4.1"
}
