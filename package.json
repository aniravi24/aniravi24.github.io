{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm feed && next build && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "feed": "node scripts/gen-rss.js",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "13.3.0",
    "nextra": "2.4.1",
    "nextra-theme-blog": "2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rss": "1.2.2",
    "sharp": "0.32.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "2.3.0",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "gh-pages": "5.0.0",
    "gray-matter": "4.0.3",
    "postcss": "8.4.23",
    "prettier": "2.8.7",
    "rehype-raw": "6.1.1",
    "remark-shiki-twoslash": "3.1.2",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  }
}
