{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm feed && next build && next-sitemap && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "feed": "node scripts/gen-rss.js",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "14.1.1",
    "next-sitemap": "4.2.3",
    "nextra": "2.13.2",
    "nextra-theme-blog": "2.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rss": "1.2.2",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@mdx-js/mdx": "3.0.0",
    "@types/node": "20.11.10",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "gh-pages": "6.1.1",
    "gray-matter": "4.0.3",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "rehype-raw": "7.0.0",
    "remark-shiki-twoslash": "3.1.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
