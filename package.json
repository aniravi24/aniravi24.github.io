{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm feed && next build && next-sitemap && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "feed": "node scripts/gen-rss.js",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.1.4",
    "next-sitemap": "4.2.3",
    "nextra": "3.3.1",
    "nextra-theme-blog": "3.3.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rss": "1.2.2",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@types/node": "22.10.5",
    "@types/react": "19.0.6",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.20",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.4",
    "gh-pages": "6.2.0",
    "gray-matter": "4.0.3",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@9.15.3"
}
