{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm feed && next build && next-sitemap && touch out/.nojekyll",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "feed": "node scripts/gen-rss.js",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.1.7",
    "next-sitemap": "4.2.3",
    "nextra": "4.2.7",
    "nextra-theme-blog": "4.2.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rss": "1.2.2",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.20.0",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.20",
    "eslint": "9.20.1",
    "eslint-config-next": "15.1.7",
    "gh-pages": "6.3.0",
    "gray-matter": "4.0.3",
    "postcss": "8.5.2",
    "prettier": "3.5.0",
    "tailwindcss": "4.0.6",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@9.15.5"
}
