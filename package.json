{
  "name": "nextra-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && next-sitemap && touch out/.nojekyll && pnpm search-build",
    "build-and-deploy": "pnpm run build && pnpm run deploy-website",
    "search-build": "pagefind --site .next/server/app --output-path out/_pagefind",
    "deploy-website": "gh-pages -d out -t true",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "15.4.6",
    "next-sitemap": "4.2.3",
    "nextra": "4.2.17",
    "nextra-theme-blog": "4.2.17",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.32.0",
    "@types/node": "22.17.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.6",
    "gh-pages": "6.3.0",
    "pagefind": "1.3.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.13.1"
}
